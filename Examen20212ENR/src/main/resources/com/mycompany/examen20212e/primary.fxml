<VBox prefHeight="606.0" prefWidth="619.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.examen20212e.PrimaryController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <GridPane alignment="TOP_CENTER" prefHeight="142.0" prefWidth="487.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="240.0" minWidth="10.0" prefWidth="143.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="347.0" minWidth="10.0" prefWidth="345.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Punto Llegada" GridPane.rowIndex="1" />
            <Label text="Oferta" GridPane.rowIndex="2" />
            <TextField fx:id="oferta" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="llegada" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label text="Punto Reoogida" />
            <TextField fx:id="recogida" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <HBox fx:id="btnConsultar" alignment="TOP_CENTER" prefHeight="50.0" prefWidth="545.0">
         <children>
            <Button fx:id="consultar" mnemonicParsing="false" text="Consultar" />
         </children>
      </HBox>
      <Pane id="panelMapa" fx:id="panelMapa" prefHeight="444.0" prefWidth="495.0" styleClass="fx-background: mapa.jpeg" stylesheets="@estilo.css" />
   </children>
</VBox>
